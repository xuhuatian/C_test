#include<stdio.h>

int main()
{
	int a = 3;
	int b = 5;
	int max = 0;
	max=(a > b ? a : b);
	/*if (a > b)
		max = a;
	else
		max = b;*/
	printf("%d\n", max);
	
	return 0;
}

